# used to remove files from deployment using `git archive`

# git files
.gitattributes export-ignore
.gitignore export-ignore

# Webstorm
.idea export-ignore

# configuration
/config export-ignore
/package.json export-ignore
.jshintrc export-ignore

# scripts
ContinuousIntegration export-ignore
deploy.sh export-ignore

# test
test export-ignore

# documentation
SolutionItems export-ignore